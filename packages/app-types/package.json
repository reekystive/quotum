{
  "name": "@quotum/app-types",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./vite": {
      "types": "./app-types-vite.d.ts",
      "default": null
    },
    "./quotum-cloudflare": {
      "types": "./quotum-cloudflare.d.ts",
      "default": null
    },
    "./quotum-nextjs-globals": {
      "types": "./quotum-nextjs-globals.d.ts",
      "default": null
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "build:tsc": "tsc --build tsconfig.json",
    "lint:eslint": "eslint . --cache --cache-strategy content --cache-location ./node_modules/.cache/eslint/eslintcache.json --max-warnings 0 --report-unused-disable-directives",
    "lint:tsc": "tsc --build tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@quotum/tsconfig": "workspace:^",
    "vite": "catalog:"
  }
}
